  "dependencies": {
    "@cmsgov/design-system": "^5.0.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.6",
    "@reach/accordion": "^0.18.0",
    "@redux-devtools/extension": "^3.3.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "4.7.0",
    "aws-amplify": "^6.15.6",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "font-awesome": "^4.7.0",
    "jsonpath": "^1.1.1",
    "launchdarkly-react-client-sdk": "^3.8.1",
    "mathjs": "^14.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-multi-select-component": "4.3.4",
    "react-redux": "^8.1.3",
    "react-router": "^7.9.1",
    "redux": "4.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "sass": "^1.92.1",
    "typescript": "^5.8.3",
    "uuid": "11.1.0",
    "vite": "6.3.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "coverage": "jest --coverage",
    "watchTest": "jest --watch",
    "pa11y": "pa11y --runner axe --runner htmlcs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "axe-core": "^4.10.3",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^9.0.0",
    "redux-mock-store": "^1.5.5",
    "ts-jest": "^29.4.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/util/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/types.js",
      "!src/config.js",
      "!src/**/index.{js,jsx}",
      "!src/components/utils/statesArray.js",
      "!src/util/testing/**/*.{js,jsx}",
      "!src/actions/**/*.{js,jsx}",
      "!src/store/**/*.{js,jsx}",
      "!src/util/idLetterMarkers.js"
    ],
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/util/testing/styleMock.js",
      "\\.(jpg|jpeg|png|gif|webp|svg|mp4|webm|wav|mp3|m4a)$": "<rootDir>/src/util/testing/mockAsset.ts"
    }
  },
  "resolutions": {
    "react-error-overlay": "6.1.0",
    "loader-utils": "^2.0.4"
  }
}
