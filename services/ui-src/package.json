    "@redux-devtools/extension": "^3.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.5.0",
    "aws-amplify": "^6.15.0",
    "date-fns": "^4.1.0",
    "font-awesome": "^4.7.0",
    "jsonpath": "^1.1.1",
    "launchdarkly-react-client-sdk": "^3.7.0",
    "mathjs": "^14.5.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-multi-select-component": "4.3.4",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.0",
    "redux": "4.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.89.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "coverage": "jest --coverage",
    "watchTest": "jest --watch",
    "pa11y": "pa11y --runner axe --runner htmlcs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "axe-core": "^4.4.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.5.1",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^8.0.0",
    "pa11y-runner-htmlcs": "^2.0.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^29.2.6"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/util/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/types.js",
      "!src/config.js",
      "!src/**/index.{js,jsx}",
      "!src/components/utils/statesArray.js",
      "!src/util/testing/**/*.{js,jsx}",
      "!src/actions/**/*.{js,jsx}",
      "!src/store/**/*.{js,jsx}",
      "!src/util/idLetterMarkers.js"
    ],
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/util/testing/styleMock.js",
      "\\.(jpg|jpeg|png|gif|webp|svg|mp4|webm|wav|mp3|m4a)$": "<rootDir>/src/util/testing/mockAsset.ts"
    }
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "loader-utils": "^2.0.4"
  }
}
